list li:nth-child(even) {\r\n    float: right;\r\n    text-align: left;\r\n    clear: both;\r\n}\r\n\r\n.timeline-content {\r\n    width: 100%;\r\n    background: #ffffff;\r\n    position: relative;\r\n}\r\n\r\n.timeline-content::after {\r\n    content: '';\r\n    position: absolute;\r\n    width: 0;\r\n    height: 0;\r\n    border-top: 10px solid transparent;\r\n    border-bottom: 10px solid transparent;\r\n    border-left: 50px solid #fff;\r\n    top: 20px;\r\n    right: -8%;\r\n    transform: rotate(30deg);\r\n}\r\n\r\n#timeline-list li:nth-child(even) .timeline-content::after {\r\n    left: -8%;\r\n    border-right: 60px solid #fff;\r\n    border-left: none;\r\n    transform: rotate(-30deg);\r\n}\r\n\r\n.timeline-content h1 {\r\n    font-family: var(--dancing-script);\r\n    font-weight: bold;\r\n    color: #0C0B0B;\r\n}\r\n\r\n.tl-img {\r\n    margin: auto;\r\n    width: 100%;\r\n}\r\n\r\n.tl-img img {\r\n    width: 100%;\r\n    height: 100%;\r\n    object-fit: cover;\r\n}\r\n\r\n.tl-desc {\r\n    color: #000;\r\n    font-family: var(--roboto);\r\n    font-weight: 500;\r\n}\r\n\r\n#timeline-list .timeline-time {\r\n    position: absolute;\r\n    top: 5%;\r\n    padding: 8px 16px;\r\n    color: #000;\r\n    font-family: var(--roboto);\r\n    text-align: center;\r\n}\r\n\r\n#timeline-list .timeline-time h5 {\r\n    font-weight: 300;\r\n}\r\n\r\n#timeline-list li:nth-child(odd) .timeline-time {\r\n    right: -50%;\r\n}\r\n\r\n#timeline-list li:nth-child(even) .timeline-time {\r\n    left: -50%;\r\n}\r\n\r\n@media screen and (max-width: 768px) {\r\n    .timeline::before {\r\n        left: -5%;\r\n    }\r\n\r\n    .timeline-content {\r\n        width: 100% !important;\r\n    }\r\n\r\n    #timeline-list li:nth-child(odd) {\r\n        float: none;\r\n        transform: translateX(0);\r\n    }\r\n    \r\n    #timeline-list li:nth-child(even) {\r\n        float: none;\r\n    }\r\n    \r\n    #timeline-list .timeline-time {\r\n        position: static;\r\n        padding